del.add(Conv2D(128, (3, 3), activation='relu'))\nmodel.add(BatchNormalization())\nmodel.add(MaxPooling2D(pool_size=(2, 2)))\nmodel.add(Dropout(0.3))\n\n# Flatten Layer\nmodel.add(Flatten())\n\n#Backed Fully Connected Layers\nmodel.add(Dense(128, activation='relu'))\nmodel.add(BatchNormalization())\nmodel.add(Dropout(0.5))\n\nmodel.add(Dense(64, activation='relu'))\nmodel.add(BatchNormalization())\nmodel.add(Dropout(0.5))\n\n#Out Layer\nmodel.add(Dense(10, activation='softmax'))\n\n#Compiling and training the model\nmodel.compile(optimizer='adam', loss='categorical_crossentropy', metrics=['accuracy'])\ntraining = model.fit(x = train.values , y = ytrain_tf, validation_split = 0.20, epochs=30, batch_size=128)\n","metadata":{"execution":{"iopub.status.busy":"2024-08-10T23:21:57.518428Z","iopub.execute_input":"2024-08-10T23:21:57.519172Z","iopub.status.idle":"2024-08-10T23:23:44.050434Z","shell.execute_reply.started":"2024-08-10T23:21:57.519133Z","shell.execute_reply":"2024-08-10T23:23:44.049341Z"},"trusted":true},"execution_count":39,"outputs":[{"name":"stdout","text":"Epoch 1/30\n\u001b[1m 22/375\u001b[0m \u001b[32m━\u001b[0m\u001b[37m━━━━━━━━━━━━━━━━━━━\u001b[0m \u001b[1m2s\u001b[0m 8ms/step - accuracy: 0.2957 - loss: 2.3739","output_type":"stream"},{"name":"stderr","text":"W0000 00:00:1723332130.408047     328 graph_launch.cc:671] Fallback to op-by-op mode because memset node breaks graph update\n","output_type":"stream"},{"name":"stdout","text":"\u001b[1m374/375\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m━\u001b[0m \u001b[1m0s\u001b[0m 7ms/step - accuracy: 0.6437 - loss: 1.0885","output_type":"stream"},{"name":"stderr","text":"W0000 00:00:1723332134.013680     326 graph_launch.cc:671] Fallback to op-by-op mode because memset node breaks graph update\n","output_type":"stream"},{"name":"stdout","text":"\u001b[1m375/375\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m16s\u001b[0m 11ms/step - accuracy: 0.6442 - loss: 1.0867 - v